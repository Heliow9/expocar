{
  "expo": {
    "name": "CheckFleetPRO",
    "slug": "RealCar52",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
      "jsEngine": "hermes",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "permissions": [
      "CAMERA",
      "LOCATION",
      "ACCESS_FINE_LOCATION",
      "ACCESS_COARSE_LOCATION",
      "ACCESS_BACKGROUND_LOCATION"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "O aplicativo precisa acessar a câmera para capturar imagens."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Este app precisa acessar sua localização em segundo plano para rastrear seu trajeto.",
        "NSLocationWhenInUseUsageDescription": "Este app precisa acessar sua localização enquanto está em uso.",
        "UIBackgroundModes": [
          "location"
        ]
      },
      "bundleIdentifier": "com.heliow99.fleetcheck"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.heliow99.fleetcheck",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
        
      ],
      "backgroundLocation": true,
      "googleMaps": {
        "apiKey": "SUA_API_KEY_AQUI"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dea51389-076d-481c-b5ff-f07e883aa03a"
      }
    },
    "owner": "heliow99"
  }
}